#include<iostream>
using namespace std;
#include <bits/stdc++.h>


int main(){
    //write your code here
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)  {
        cin>>a[i];
    }
    int m=*max_element(a,a+n);
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
        if(a[j]<m-i){
            cout<<"\t";
        }
        else{
            cout<<"*\t";
        }
    }
    cout<<endl;
        
    }
    
    
    
}
