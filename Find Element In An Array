#include<iostream>
using namespace std;


int main(){
    //write your code here
    int n,d;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    cin>>d;
    int ctr=-1;
    for(int i=0;i<n;i++){
    
        if(a[i]==d){
            ctr=i;
            break;
        }
    }
    cout<<ctr;
}
